    private void resetFilterKelola() {
        if (sorterKelola != null) {
            sorterKelola.setSortKeys(null);
            sorterKelola.setRowFilter(null);
        }
        if (JCariAkun != null) {
            JCariAkun.setText("");
        }
        if (jFilterKelolaAkun != null) {
            jFilterKelolaAkun.setSelectedIndex(0);
        }
    }
    
    public void refreshLaporan() {
        loadDataLaporan();
        resetFilterLaporan();
        showPanel(LaporanPetugas);
    }
    
    public void refreshRekam() {
        loadDataRekam();
        resetFilterRekam();
    }
    
    public void refreshKelola() {
        loadDataKelola();
        resetFilterKelola();
        RefreshDashboard();
    }
    
