    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton2ActionPerformed
         int selectedRow = tabelLaporan.getSelectedRow();

    if (selectedRow == -1) {
        JOptionPane.showMessageDialog(this,
            "Pilih data laporan yang ingin diubah!",
            "Peringatan",
            JOptionPane.WARNING_MESSAGE);
        return;
    }

    int idLaporan = (int) modelLaporan.getValueAt(selectedRow, 0);
    String keluhanLama = (String) modelLaporan.getValueAt(selectedRow, 4);
    String tindakanLama = (String) modelLaporan.getValueAt(selectedRow, 5);

    // Buat panel custom untuk 2 input
    JTextArea inputKeluhan = new JTextArea(3, 20);
    JTextArea inputTindakan = new JTextArea(3, 20);
    inputKeluhan.setText(keluhanLama);
    inputTindakan.setText(tindakanLama);

    JPanel panel = new JPanel();
    panel.setLayout(new BoxLayout(panel, BoxLayout.Y_AXIS));
    panel.add(new JLabel("Keluhan / Diagnosis:"));
    panel.add(new JScrollPane(inputKeluhan));
    panel.add(Box.createVerticalStrut(10)); // jarak
    panel.add(new JLabel("Tindakan:"));
    panel.add(new JScrollPane(inputTindakan));

    int result = JOptionPane.showConfirmDialog(this, panel, 
            "Ubah Data Laporan", JOptionPane.OK_CANCEL_OPTION, JOptionPane.PLAIN_MESSAGE);

    if (result == JOptionPane.OK_OPTION) {
        String keluhanBaru = inputKeluhan.getText().trim();
        String tindakanBaru = inputTindakan.getText().trim();

        if (keluhanBaru.isEmpty() || tindakanBaru.isEmpty()) {
            JOptionPane.showMessageDialog(this, "Semua field harus diisi!", "Error", JOptionPane.ERROR_MESSAGE);
            return;
        }

        boolean success = laporanService.ubahLaporanKesehatan(idLaporan, keluhanBaru, tindakanBaru);

        if (success) {
            JOptionPane.showMessageDialog(this, " Data berhasil diubah!");
            loadDataLaporan(); // refresh tabel
        } else {
            JOptionPane.showMessageDialog(this, " Gagal mengubah data!", "Error", JOptionPane.ERROR_MESSAGE);
        }
    }
    }//GEN-LAST:event_jButton2ActionPerformed

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton3ActionPerformed
         int selectedRow = tabelLaporan.getSelectedRow();
        
