    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton5ActionPerformed
        int selectedRow = tableAkun.getSelectedRow();
        if (selectedRow == -1) {
            JOptionPane.showMessageDialog(this, 
                "Silakan pilih akun yang ingin diubah dari tabel terlebih dahulu.", 
                "Peringatan", 
                JOptionPane.WARNING_MESSAGE);
            return;
        }

        // Ambil data dari tabel (pastikan urutan kolom sesuai tabelAkun kamu)
        int idUser = Integer.parseInt(tableAkun.getValueAt(selectedRow, 0).toString());
        String username = tableAkun.getValueAt(selectedRow, 1).toString();
        String role = tableAkun.getValueAt(selectedRow, 2).toString();
        String nama = tableAkun.getValueAt(selectedRow, 3).toString();

        // --- Input baru dari user ---
        String usernameBaru = JOptionPane.showInputDialog(this, "Username baru:", username);
        if (usernameBaru == null) return;

        String passwordBaru = JOptionPane.showInputDialog(this, "Password baru:");
        if (passwordBaru == null) return;

        String namaBaru = JOptionPane.showInputDialog(this, "Nama baru:", nama);
        if (namaBaru == null) return;

        String bagianAtauSpesialis = "";
        if ("Petugas".equalsIgnoreCase(role)) {
            bagianAtauSpesialis = JOptionPane.showInputDialog(this, "Bagian Petugas:");
        } else if ("Dokter".equalsIgnoreCase(role)) {
            bagianAtauSpesialis = JOptionPane.showInputDialog(this, "Spesialis Dokter:");
        }

        // --- Panggil Service untuk update ---
        ServiceUser serviceUser = new ServiceUser();
        boolean sukses = serviceUser.updateAkun(
            idUser,
            usernameBaru,
            passwordBaru,
            namaBaru,
            bagianAtauSpesialis,
            role
        );

        if (sukses) {
            JOptionPane.showMessageDialog(this, 
                " Akun berhasil diperbarui!", 
                "Sukses", 
                JOptionPane.INFORMATION_MESSAGE);
            loadDataKelola(); // Refresh tabel (pastikan method ini ada)
        } else {
            JOptionPane.showMessageDialog(this, 
                " Gagal memperbarui akun.", 
                "Error", 
                JOptionPane.ERROR_MESSAGE);
        }
    }//GEN-LAST:event_jButton5ActionPerformed

    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton6ActionPerformed
        int selectedRow = tableAkun.getSelectedRow();

            if (selectedRow == -1) {
                JOptionPane.showMessageDialog(this,
                    "Pilih akun yang ingin dihapus!",
                    "Peringatan",
                    JOptionPane.WARNING_MESSAGE);
                return;
            }

            int idUser = (int) modelKelola.getValueAt(selectedRow, 0);
            String username = (String) modelKelola.getValueAt(selectedRow, 1);
            String role = (String) modelKelola.getValueAt(selectedRow, 2);
            String nama = (String) modelKelola.getValueAt(selectedRow, 3);

            // Cegah hapus akun sendiri
            if (idUser == this.idPetugasLogin) { //  PAKE idPetugasLogin
                JOptionPane.showMessageDialog(this,
                    "Tidak dapat menghapus akun sendiri!",
                    "Error",
                    JOptionPane.ERROR_MESSAGE);
                return;
            }

            int confirm = JOptionPane.showConfirmDialog(this,
                "Apakah Anda yakin ingin menghapus akun:\n" + 
                "Username: " + username + "\n" +
                "Nama: " + nama + "\n" +
                "Role: " + role + "?",
                "Konfirmasi Hapus",
                JOptionPane.YES_NO_OPTION,
                JOptionPane.WARNING_MESSAGE);

            if (confirm == JOptionPane.YES_OPTION) {
                try {
                    java.sql.Connection conn = Model.KoneksiEldercare.getConnection();

                    // Hapus dari tabel role-specific dulu (foreign key)
                    if (role.equals("Dokter")) {
                        String sqlDokter = "DELETE FROM dokter WHERE id_user = ?";
                        java.sql.PreparedStatement pstmtDokter = conn.prepareStatement(sqlDokter);
                        pstmtDokter.setInt(1, idUser);
                        pstmtDokter.executeUpdate();
                        pstmtDokter.close();
                    } else if (role.equals("Petugas")) {
                        String sqlpetugas_puskesmas = "DELETE FROM petugas_puskesmas WHERE id_user = ?";
                        java.sql.PreparedStatement pstmtpetugas_puskesmas = conn.prepareStatement(sqlpetugas_puskesmas);
                        pstmtpetugas_puskesmas.setInt(1, idUser);
                        pstmtpetugas_puskesmas.executeUpdate();
                        pstmtpetugas_puskesmas.close();
                    }

                    // Hapus dari tabel user
                    String sqlUser = "DELETE FROM user WHERE id_user = ?";
                    java.sql.PreparedStatement pstmtUser = conn.prepareStatement(sqlUser);
                    pstmtUser.setInt(1, idUser);
                    int result = pstmtUser.executeUpdate();
                    pstmtUser.close();

                    if (result > 0) {
                        JOptionPane.showMessageDialog(this, 
                            "Akun berhasil dihapus!", 
                            "Sukses", 
                            JOptionPane.INFORMATION_MESSAGE);

                        loadDataKelola(); // Refresh tabel
                    }

                } catch (Exception e) {
                    JOptionPane.showMessageDialog(this, 
                        "Gagal menghapus akun: " + e.getMessage(), 
                        "Error", 
                        JOptionPane.ERROR_MESSAGE);
                    e.printStackTrace();
                }
            }
    }//GEN-LAST:event_jButton6ActionPerformed

