    private void searchKelolaAkun() {
        if (sorterKelola == null || JCariAkun == null) return;
        
        try {
            String searchText = JCariAkun.getText().trim();
            if (searchText.isEmpty()) {
                sorterKelola.setRowFilter(null);
            } else {
                sorterKelola.setRowFilter(RowFilter.regexFilter("(?i)" + searchText));
            }
        } catch (Exception e) {
            logger.warning("Error in searchKelolaAkun: " + e.getMessage());
        }
    }
    
