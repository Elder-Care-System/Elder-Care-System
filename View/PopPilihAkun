    private void jPanel6MouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jPanel6MouseClicked
        if (menuPetugas != null) {
            // Buat panel tambah akun petugas
            TambahAkunPetugas panelPetugas = new TambahAkunPetugas();
            
            // Tampilkan di frame utama MenuPetugas
            menuPetugas.showPanel(panelPetugas);
            
            // Tutup popup
            this.dispose();
        } else {
            System.err.println("Error: MenuPetugas belum diinisialisasi!");
        }
    }//GEN-LAST:event_jPanel6MouseClicked

    private void jPanel5MouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jPanel5MouseClicked
        if (menuPetugas != null) {
            // Buat panel tambah akun dokter
            TambahAkunDokter panelDokter = new TambahAkunDokter();
            
            // Tampilkan di frame utama MenuPetugas
            menuPetugas.showPanel(panelDokter);
            
            // Tutup popup
            this.dispose();
        } else {
            System.err.println("Error: MenuPetugas belum diinisialisasi!");
        }
    }//GEN-LAST:event_jPanel5MouseClicked

